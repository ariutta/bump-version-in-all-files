{
  "name": "semver-in-file-text-bumper",
  "version": "2.2.3",
  "description": "Utility for bumping version number (e.g., 1.2.3 to 1.3.0) in the text of all appropriate files.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ariutta/semver-in-file-text-bumper.git"
  },
  "keywords": [
    "bump"
  ],
  "author": "Anders Riutta",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ariutta/semver-in-file-text-bumper/issues"
  },
  "homepage": "https://github.com/ariutta/semver-in-file-text-bumper#readme",
  "dependencies": {
    "JSONStream": "^1.0.6",
    "colors": "^1.1.2",
    "fstream-npm": "^1.0.7",
    "lodash": "^3.10.1",
    "resolve-from": "^1.0.1",
    "rx": "^4.0.6",
    "rx-fs": "https://github.com/ariutta/rx-fs.git",
    "rx-node": "^1.0.1",
    "rx-node-extra": "https://github.com/ariutta/rx-node-extra.git",
    "semver-inc-wizard": "https://github.com/ariutta/semver-inc-wizard.git",
    "shelly": "0.0.3",
    "strip-ansi": "^3.0.0"
  },
  "devDependencies": {
    "bdd-stdin": "^0.2.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "colors": "^1.1.2",
    "lodash": "^3.10.1",
    "mocha": "^2.3.4",
    "mockery": "^1.4.0",
    "sinon": "^1.17.2",
    "util": "^0.10.3"
  },
  "semverInFileTextBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [],
      "excludeDir": ["test"]
    }
  }
}
